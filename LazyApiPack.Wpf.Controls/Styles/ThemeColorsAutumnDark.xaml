    <!-- Theme: Nature -->
    <!--<Color x:Key="PrimaryForegroundColor">Black</Color>
    <Color x:Key="PrimaryColorA" >#47FD53</Color>
    <Color x:Key="PrimaryColorB" >#6BE340</Color>
